  "scripts": {
    "start": "set NODE_ENV=development && react-app-rewired start && npm run api && npm run devtrans",
    "build": "npm run trans & react-app-rewired build",
    "server": "npm run build && serve -s build",
    "test": "react-app-rewired test --env=jsdom",
    "test-all": "CI=true npm run test -- --coverage",
    "eject": "react-app-rewired eject",
    "api": "nodemon --watch api --exec babel-node api --presets es2015,stage-2",
