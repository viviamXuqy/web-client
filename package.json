    "stylelint-order": "^1.0.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-ftp-upload": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "setupFiles": [
      "jest-canvas-mock",
      "jest-localstorage-mock"
